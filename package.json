{
  "name": "sempro2",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
